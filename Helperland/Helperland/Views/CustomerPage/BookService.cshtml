
@{
    ViewData["Title"] = "SetupService";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Helperland | Set Up Cleaning Service</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="stylesheet" href="/css/style.css">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@500&display=swap" rel="stylesheet">

    <script src="https://kit.fontawesome.com/5602f8a8c9.js" crossorigin="anonymous"></script>

</head>
<body onload="noBack()" onpageshow="if (event.persisted) noBack();" onunload="">


    <!-- nav starts -->
    @await Html.PartialAsync("FaqNav")
    <!-- nav ends -->
    <!-- header starts -->
    <header>

        <!-- set up service banner starts -->
        <div>
            <img src="/img/book-service-banner.jpg" id="banner" alt="">
        </div>
        <!-- set up service banner ends -->
        <!-- title of page starts -->
        <div class="text-center mt-4">
            <h1>Set up your cleaning service</h1>
        </div>
        <!-- title of page ends -->
        <!-- title bottom design starts -->
        <div class="title-bottom-design">
            <hr>
            <div class="text-center">
                <img src="/img/faq/separator.png" alt="">
            </div>
            <hr>
        </div>
        <!-- title bottom design ends -->

    </header>
    <!-- header ends -->
    <!-- main starts -->
    <main id="bookServiceMainTag">

        <!-- main section starts -->
        <section>
            <div id="setUpServiceMain">

                <!-- form section starts -->
                <div id="set-up-formdiv">

                    <!-- set up form steps -->
                    <div id="book-now-steps">
                        <div class="booknow_tab" id="setup-service" role="button" onclick="form1();">
                            <span><img src="/img/setup-service-white.png" alt=""></span>
                            <span id="form1Span">Setup Service</span>
                            <div class="downarrow" id="form1Down"></div>
                        </div>
                        <div class="booknow_tab" id="schedule-service" onclick="form2();" role="button">
                            <span><img src="/img/schedule.png" alt="" id="form2Img"></span>
                            <span id="form2Span">Schedule & Plan</span>
                            <div class="downarrow" id="form2Down"></div>
                        </div>
                        <div class="booknow_tab" id="details-service" onclick="form3();" role="button">
                            <span><img src="/img/details.png" alt="" id="form3Img"></span>
                            <span id="form3Span">Your Details</span>
                            <div class="downarrow" id="form3Down"></div>
                        </div>
                        <div class="booknow_tab" id="payment-service" onclick="form4();" role="button">
                            <span><img src="/img/payment.png" alt="" id="form4Img"></span>
                            <span id="form4Span">Make Payment</span>
                            <div class="downarrow" id="form4Down"></div>
                        </div>
                    </div>
                    <!-- set up form steps ends -->

                    <form asp-controller="CustomerPage" asp-action="validPostcode" id="setupform">
                        <div id="form1">
                            <h4>Enter your postal code</h4>
                            <input type="text" id="postalCode" placeholder="Postal Code">
                            <span></span>
                            <button type="button">Check Availibility</button>
                        </div>
                    </form>
                    <form action="" id="setupform2">
                        <div id="form2">

                            <!-- datetime and how long starts -->
                            <div id="datetimeAndHowlong">
                                <!-- date and time starts -->
                                <div id="book-datetime">
                                    <h4>When do you need the cleaner?</h4>
                                    <span id="fdate">
                                        <input type="date" id="admin-sr-fdatepicker" name="fromdate" onchange="writefDate();">
                                        <input type="text" name="fromdate" id="admin-sr-fdate" placeholder="Enter Date" onblur="if(this.value){this.type='date';}else{this.type='text';}" onfocus="this.type='date'">
                                    </span>
                                    <select name="service-time" id="startingtime">
                                        <option value="1:00" selected>1:00 PM</option>
                                        <option value="2:00">2:00 PM</option>
                                        <option value="3:00">3:00 PM</option>
                                        <option value="4:00">4:00 PM</option>
                                        <option value="5:00">5:00 PM</option>
                                    </select>
                                </div>
                                <!-- date and time ends -->
                                <!-- how long starts -->
                                <div id="">
                                    <h4>How long do you need your cleaner to stay?</h4>
                                    <select name="service-hours" id="servicehours">
                                        <option value="1" selected>1.0 Hrs</option>
                                        <option value="2">2.0 Hrs</option>
                                        <option value="3">3.0 Hrs</option>
                                        <option value="4">4.0 Hrs</option>
                                        <option value="5">5.0 Hrs</option>
                                    </select>
                                </div>
                                <!-- how long ends -->
                            </div>
                            <!-- datetime and how long ends -->

                            <h4>Extra services</h4>
                            <div id="custom-checkboxes">
                                <div class="checkbox">
                                    <input type="checkbox" name="insideCabinet" id="insideCabinetCheck" class="htmlcheckbox" onclick="onInsideCabinet();">
                                    <label for="insideCabinetCheck" onclick="onInsideCabinet();"><img src="/img/3.png" id="insideCabinetImg" alt=""></label>
                                    <p>inside cabinets</p>
                                </div>

                                <div class="checkbox">
                                    <input type="checkbox" id="insideFridgeCheck" name="insideFridge" class="htmlcheckbox" onclick="onInsideFridge();">
                                    <label for="insideFridgeCheck"><img src="/img/5.png" id="insideFridgeImg" alt="" onclick="onInsideFridge();"></label>
                                    <p>inside Fridge</p>
                                </div>

                                <div class="checkbox">
                                    <input type="checkbox" name="insideOven" id="insideOvenCheck" class="htmlcheckbox" onclick="onInsideOven();">
                                    <label for="insideOvenCheck" onclick="onInsideOven();"><img src="/img/4.png" id="insideOvenImg" alt=""></label>
                                    <p>inside Oven</p>
                                </div>

                                <div class="checkbox">
                                    <input type="checkbox" id="laundryCheck" name="laundry" class="htmlcheckbox" onclick="onLaundry();">
                                    <label for="laundryCheck" onclick="onLaundry();"><img src="/img/2.png" id="laundryImg" alt=""></label>
                                    <p>Laundry Wash & Dry</p>
                                </div>

                                <div class="checkbox">
                                    <input type="checkbox" id="interiorCheck" name="interior" class="htmlcheckbox" onclick="onInterior();">
                                    <label for="interiorCheck" onclick="onInterior();"><img src="/img/1.png" id="interiorImg" alt=""></label>
                                    <p>interior Windows</p>
                                </div>
                            </div>

                            <h4>Comments</h4>

                            <textarea name="comments" id="comments" placeholder="Comments..." cols="30" rows="10"></textarea>
                            <div id="ihavepet">
                                <input type="checkbox" name="havepet" id="havepet">
                                <span>I have pets at home</span>
                            </div>

                            <button id="form2continue" type="button" onclick="form3();">continue</button>
                        </div>


                        <div id="form3">

                            <h4>Enter your contact details, so we can serve you in better way!</h4>

                            <!-- addresses starts -->
                            <div id="addresses">
                                <div>
                                    <input type="radio" name="address" id="address1">
                                    <label for="address1">
                                        <p><b>Address:</b> street 2 40, Troisdorf 53844</p>
                                        <p><b>Phone number:</b> 8200035128</p>
                                    </label>
                                </div>
                                <div>
                                    <input type="radio" name="address" id="address2">
                                    <label for="address2">
                                        <p><b>Address:</b> street 2 40, Troisdorf 53844</p>
                                        <p><b>Phone number:</b> 8200035128</p>
                                    </label>
                                </div>

                            </div>
                            <!-- addresses ends -->
                            <!-- add address starts -->

                            <p id="addnewAddressA" onclick="addAddressdiv();">+ Add New Addresses</p>

                            <div id="addAddressDiv">
                                <div>
                                    <div>
                                        <p>Street name</p>
                                        <input type="text" name="addStreetname" id="addStreetname" placeholder="Street name">
                                    </div>
                                    <div>
                                        <p>House number</p>
                                        <input type="text" name="addHouseno" id="addHouseno" placeholder="House number">
                                    </div>
                                </div>
                                <div>
                                    <div>
                                        <p>Postal Code</p>
                                        <input type="text" name="addPostalcode" placeholder="Postal Code" id="addPostalcode" disabled>
                                    </div>
                                    <div>
                                        <p>City</p>
                                        <Select name="addCity" id="addCity">
                                            <option value="#">Select City</option>
                                            <option value="Bhavnagar">Bhavnagar</option>
                                            <option value="Ahemdabad">Amdavad</option>
                                            <option value="Surat">Surat</option>
                                        </Select>
                                    </div>
                                </div>
                                <div>
                                    <div>
                                        <p>Phone number</p>
                                        <span></span>
                                        <input type="text" name="addPhoneno" id="addPhoneno">
                                    </div>
                                </div>
                                <div>
                                    <button type="button" id="addaddressbtn" onclick="saveAddress();">Save</button>
                                    <button type="button" id="resetaddressbtn" onclick="cancelAddAddress();">Cancel</button>
                                </div>
                            </div>

                            <div id="sendInvoicediv">
                                <input type="checkbox" name="sendInvoice" id="sendInvoice" onchange="displayInvoiceAd();">
                                <label for="sendInvoice">Send invoice on this address</label>


                                <!-- invoice address starts -->
                                <div id="invoiceAddressDiv">
                                    <div>
                                        <div>
                                            <p>Street name</p>
                                            <input type="text" name="invoiceStreetname" id="invoiceStreetname" placeholder="Street name">

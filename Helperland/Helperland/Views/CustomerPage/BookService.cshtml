                                </div>
                                <div>
                                    <span>Inside cabinets (extra)</span>
                                    <span>30 mins</span>
                                </div>

                                <div id="total-time">
                                    <span><b>Total Service Time</b></span>
                                    <span><b>3.5 hrs</b></span>
                                </div>
                            </div>
                            <!-- service time ends -->
                            <!-- cost strats -->
                            <div id="service-cost" class="flex-column">
                                <div>
                                    <span>Per cleaning</span>
                                    <span><b>&dollar;87</b></span>
                                </div>

                                <div>
                                    <span>Discount</span>
                                    <span><b>- &dollar;27</b></span>
                                </div>
                            </div>
                            <!-- cost ends -->
                            <!-- payment starts-->
                            <div id="service-pay" class="flex-column">
                                <div>
                                    <span id="total-pay">Total Payment</span>
                                    <span id="amount">&dollar;63</span>
                                </div>
                                <div>
                                    <span>Effective Price</span>
                                    <span id="last-amount"><b>&dollar;50.4</b></span>
                                </div>

                            </div>

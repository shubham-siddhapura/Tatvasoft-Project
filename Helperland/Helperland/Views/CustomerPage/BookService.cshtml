                                            <label for="sandipSP">Select</label>
                                        </div>
                                        <div id="favSP2" class="favSP">
                                            <img src="/img/customer-serviceHistory/cap.png" alt="">
                                            <p>Abcd Xyz</p>
                                            <input type="radio" name="favSP" value="abcd" id="abcdSP">
                                            <label for="abcdSP">Select</label>
                                        </div>
                                    </div>
                                </div>
                                <!-- favourite service provider ends -->

                            </div>
                            <button id="form3continue" type="button" onclick="form4();">continue</button>

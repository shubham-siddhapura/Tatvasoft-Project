

@{
    ViewData["Title"] = "CustServiceHistory";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Helperland | Service History</title>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:wght@500&display=swap" rel="stylesheet">

    <script src="https://kit.fontawesome.com/5602f8a8c9.js" crossorigin="anonymous"></script>

    <link rel="stylesheet"
          type="text/css"
          href="https://cdn.datatables.net/1.11.3/css/jquery.dataTables.css" />

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.2/main.min.css">

    <script type="text/javascript"
            charset="utf8"
            src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.js"></script>

    <link rel="stylesheet" href="/css/style.css">

</head>
<body>

    @await Html.PartialAsync("FaqNav")

    <!-- main starts -->
    <main>
        <!-- header starts -->
        <section id="upcoming-header">
            <h1>
                <span>Welcome,</span> @TempData["Name"]
            </h1>
        </section>
        <!-- header ends -->
        <!-- main section starts -->
        <section>
            <div id="upcoming-main">

                <div id="upcoming-desktop-flex">
                    <div id="upcoming-menu-desktop">
                        <ul>
                            <li id="dashbordTab" onclick="dashbord()" class="active"> Dashbord</li>
                            <li id="serviceHistoryTab" onclick="serviceHistory()">Service History</li>
                            <li id="ServiceScheduleTab" role="button">Service Schedule</li>
                            <li id="favouriteProsTab" role="button">Favourite Pros</li>
                            <li>Invoices</li>
                            <li>Notification</li>
                        </ul>

                    </div>

                    @await Html.PartialAsync("CustomerDashbord")

                    @await Html.PartialAsync("CustomerServiceHistory")

                    @await Html.PartialAsync("CustomerMySetting")

                    @await Html.PartialAsync("CustomerFavouritePros")

                    @await Html.PartialAsync("ServiceSchedule")


                </div>


            </div>
        </section>
        <!-- main section ends -->

        @await Html.PartialAsync("CustomerModals")

    </main>
    <!-- main ends -->

    @await Html.PartialAsync("Footer")

    @await Html.PartialAsync("LoadingAnimation")

    @*@await Html.PartialAsync("CustomerModals")*@

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>


    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script type="text/javascript"
            src="https://cdn.datatables.net/v/dt/dt-1.11.3/r-2.2.9/rg-1.1.4/datatables.min.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.1.0/js/dataTables.buttons.min.js"></script>
    <script src="https://cdn.datatables.net/buttons/2.1.0/js/buttons.html5.min.js"></script>

    <script type="text/javascript" src="https://unpkg.com/xlsx@0.15.1/dist/xlsx.full.min.js"></script>

    <script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.10.2/main.min.js"></script>

    <script src="~/js/CustomDate.min.js"></script>

    <script src="~/js/CustomerPage.js"></script>

    <script src="~/js/ServiceSchedule.js"></script>

    <script src="~/js/CustomerFavouritePros.js"></script>

    @*<script>
            window.history.forward();
            function noBack() {
                window.history.forward();
            }
        </script>*@

</body>
</html>
<form  id="setupform" class="setupform">
    <div id="form1">
        
        <div class="alert alert-danger d-none mt-3" id="postalcodeAlert" role="alert">
        </div>
        <h4>Enter your postal code</h4>
        <input type="text" asp-for="PostalCode" id="postalCode" placeholder="Postal Code" value=@TempData["PostalCode"]>


        <button type="button" onclick="postalSubmit()">Check Availibility</button>
    </div>
</form>